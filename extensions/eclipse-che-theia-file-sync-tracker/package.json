{
  "name": "@eclipse-che/theia-file-sync-tracker",
  "keywords": [
    "theia-extension"
  ],
  "version": "7.21.2",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "1.7.0-next.b3aa18fa",
    "@eclipse-che/theia-remote-api": "7.21.2"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "clean": "rimraf lib",
    "format": "tsfmt -r --useTsfmt ../../configs/tsfmt.json",
    "lint": "eslint --cache=true --no-error-on-unmatched-pattern=true \"{src,test}/**/*.{ts,tsx}\"",
    "compile": "tsc",
    "build": "concurrently -n \"format,lint,compile\" -c \"red,green,blue\" \"yarn format\" \"yarn lint\" \"yarn compile\"",
    "watch": "tsc -w"
  },
  "license": "EPL-2.0",
  "theiaExtensions": [
    {
      "frontend": "lib/browser/che-theia-file-sync-frontend-module"
    }
  ]
}
